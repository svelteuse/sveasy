{
  "name": "sveasy",
  "version": "0.6.4",
  "homepage": "https://github.com/alexanderniebuhr/sveasy#readme",
  "bugs": {
    "url": "https://github.com/alexanderniebuhr/sveasy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexanderniebuhr/sveasy.git"
  },
  "license": "ISC",
  "author": "Alexander Niebuhr",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./browser": {
      "require": "./lib/browser/browser.js",
      "import": "./lib/browser/browser.mjs"
    }
  },
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "node": [
        "./lib/browser.d.ts"
      ]
    }
  },
  "bin": "bin/sveasy.js",
  "files": [
    "lib/**/*",
    "bin/**/*"
  ],
  "scripts": {
    "prepublishOnly": "rimraf lib && npm run build",
    "build": "tsup src/index.ts src/browser/browser.ts --dts --format esm,cjs --out-dir lib --external node:fs --external node:path --external node:http",
    "dev": "npm run build -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sort": "npx sort-package-json@latest",
    "check": "npx taze@latest major"
  },
  "dependencies": {
    "esbuild": "0.12.19"
  },
  "devDependencies": {
    "@nbhr/utils": "0.0.29",
    "@types/node": "16.4.13",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-regexp": "0.13.2",
    "eslint-plugin-sonarjs": "0.9.1",
    "eslint-plugin-unicorn": "35.0.0",
    "get-port": "5.1.1",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "svelte": "3.42.1",
    "tsup": "4.13.1",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">14.x"
  }
}
