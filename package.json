{
  "name": "sveasy",
  "version": "0.7.3",
  "homepage": "https://github.com/alexanderniebuhr/sveasy#readme",
  "bugs": {
    "url": "https://github.com/alexanderniebuhr/sveasy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexanderniebuhr/sveasy.git"
  },
  "license": "ISC",
  "author": {
    "name": "Alexander Niebuhr",
    "email": "tech@nbhr.io"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./browser": {
      "require": "./dist/browser/browser.js",
      "import": "./dist/browser/browser.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "browser": [
        "./dist/browser.d.ts"
      ]
    }
  },
  "bin": "bin/sveasy.js",
  "files": [
    "dist/**/*",
    "bin/**/*"
  ],
  "scripts": {
    "prepublishOnly": "rimraf lib && npm run build",
    "build": "tsup src/index.ts src/browser/browser.ts --dts --format esm,cjs --out-dir dist --external node:fs --external node:path --external node:http",
    "dev": "npm run build -- --watch"
  },
  "dependencies": {
    "esbuild": "0.15.0"
  },
  "devDependencies": {
    "@alexanderniebuhr/style": "1.0.13",
    "@nbhr/utils": "0.0.29",
    "@types/node": "16.10.2",
    "@types/yargs": "17.0.3",
    "get-port": "5.1.1",
    "rimraf": "3.0.2",
    "svelte": "3.43.1",
    "tsup": "5.2.1",
    "typescript": "4.4.3",
    "windicss": "3.1.8",
    "yargs": "17.2.1"
  },
  "engines": {
    "node": ">14.x"
  },
  "volta": {
    "node": "16.10.0",
    "npm": "7.24.1"
  }
}
