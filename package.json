{
  "name": "sveasy",
  "version": "0.5.0",
  "homepage": "https://github.com/alexanderniebuhr/sveasy#readme",
  "author": "Alexander Niebuhr",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexanderniebuhr/sveasy.git"
  },
  "bugs": {
    "url": "https://github.com/alexanderniebuhr/sveasy/issues"
  },
  "license": "ISC",
  "bin": "bin/sveasy.js",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./browser": {
      "require": "./lib/browser/browser.js",
      "import": "./lib/browser/browser.mjs"
    }
  },
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "node": [
        "./lib/browser.d.ts"
      ]
    }
  },
  "files": [
    "lib/**/*",
    "bin/**/*"
  ],
  "scripts": {
    "prepublishOnly": "rimraf lib && npm run build",
    "build": "tsup src/index.ts src/browser/browser.ts --dts --format esm,cjs --out-dir lib",
    "dev": "npm run build -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "esbuild": "0.12.14"
  },
  "devDependencies": {
    "@nbhr/utils": "0.0.29",
    "@types/node": "16.0.0",
    "rimraf": "3.0.2",
    "svelte": "3.38.3",
    "ts-standard": "10.0.0",
    "tsup": "4.12.0",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">14.x"
  }
}